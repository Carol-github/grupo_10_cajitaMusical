        <section class="slider_mobile">
            <img style="width: 100%;"  src="/img/slider_cm_1_mobile.jpeg" alt="">
        </section>
        <h3 style="margin-top: .7%; margin-bottom: 1%; text-align: center; font-weight: 300;">CATEGORIAS</h3>
        <section style="display:flex; justify-content: space-evenly;">
            <div style="width: 22%; border-style: solid; border-color: black; border-width: 1px;">
                <img style="width: 100%;" src="/img/cat_guitar_color.jpg" alt="">
                <div style="padding-left:5%;">
                    <h4>CUERDAS</h4>
                    <p style="font-weight: 300;">Guitarras, Bajos, Violines, Contrabajos ...</p>
                </div>
            </div>
            <div style="width: 22%; border-style: solid; border-color: black; border-width: 1px;">
